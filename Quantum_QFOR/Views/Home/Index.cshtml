@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using Quantum_QFOR;

<div ng-app="" ng-controller="AdministratorController">
    <table>
        <tr ng-repeat="menuitem in menuitems">
            <td>{{ menuitem.Id }}</td>
            <td>{{ menuitem.MenuName }}</td>
        </tr>
    </table>
</div>

<script>
    function AdministratorController($scope, $http) {
        varurl: '@Url.Action("AdministratorController", "GetMenuItems")', dataType: "json",
        $http.get(url).success(function (response) {
            $scope.menuitems = response;
        });
    }
</script>

